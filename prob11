#include <iostream>
using namespace std;
int main() {
    double w1,p1,w2,p2;
    cout<<"Enter the  weight and price of package 1 ";
    cin>>w1>>p1;
    cout<<"Enter the  weight and price of package 2 ";
    cin>>w2>>p2;
     double cost1=w1/p1;
    double cost2=w2/p2;
    if(cost1>cost2) {
        cout<<"Package 1 has a better price than package 2"<<endl;
    }
    else if (cost1<cost2) {
        cout<<"Package 2 has a better price than package 1"<<endl;
    }
    else {
        cout<<"Both of the them have the same price"<<endl;
    }
    return 0;
}
