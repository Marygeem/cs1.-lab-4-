#include <iostream>
#include <cmath>
using namespace std;
int main() {
    double x,y;
    cout<<"Enter point coordinates (x,y)";
    cin>>x>>y;
    double distance=sqrt(x*x+y*y);
    if (distance <=10) {
        cout<<"The points are inside the circle";
    }
    else {
        cout<<"The points are not inside the circle";
    }
    return 0;
}
