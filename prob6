#include <iostream>
#include <cmath>
using namespace std;
int main () {
    double a,b,c;
    cout<<"Enter a,b,c ";
    cin>>a>>b>>c;
    double d=b*b-4*a*c;
    if(d>0) {
        cout<<"x1="<<(-b+sqrt(d)/2*a)<<endl;
        cout<<"x2="<<(-b-sqrt(d)/2*a)<<endl;
    }
    else if (d==0) {
        cout<<"x="<<(-b/2*a)<<endl;
    }
    else {
        cout<<"No Real Solution"<<endl;
    }
    return 0;
}
